{
    "name": "interfrog/if_sporty_content",
    "type": "typo3-cms-extension",
    "version": "1.0.0",
    "description": "TYPO3 extension with content for IfPage Sporty",
    "keywords": [
        "TYPO3",
        "extension",
        "if_basic_content"
    ],
    "homepage": "http://interfrog.de",
    "authors": [{
        "name": "Interfrog Produktion GmbH",
        "role": "Developer",
        "homepage": "http://interfrog.de"
    }],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": ">=7.6,<8.0"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
